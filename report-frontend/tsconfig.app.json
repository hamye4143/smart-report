{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src", //
    "paths": {
      "src/*": ["./*"],                // ✅ 이 줄 추가!
      "constants/*": ["constants/*"],
      "environments/*": ["environments/*"]
    }
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}

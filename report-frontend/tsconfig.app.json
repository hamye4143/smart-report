{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "src/*": ["./*"],
      "constants/*": ["constants/*"],
      "environments/*": ["environments/*"]
    }
  },
  "angularCompilerOptions": {
    "strictTemplates": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
